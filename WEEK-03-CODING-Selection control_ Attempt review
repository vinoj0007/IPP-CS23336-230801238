  .dashboard-card-deck:not(.fixed-width-cards) .dashboard-card { width: calc(50% - 0.5rem); }
  .blocks-post .dashboard-card-deck .dashboard-card, .blocks-pre .dashboard-card-deck .dashboard-card { width: calc(100% - 0.5rem); }
}

